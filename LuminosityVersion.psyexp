 green=1&amp;#10;#    r = 0&amp;#10;#    g = 1&amp;#10;#    b = trial_value&amp;#10;&amp;#10;#elif condition == 3 :&amp;#10;#    red = random&amp;#10;#    blue=1&amp;#10;#    green=0&amp;#10;#    r = trial_value&amp;#10;#    g = 0&amp;#10;#    b = 1&amp;#10;&amp;#10;color=lab2rgb((L,A,B))&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="thisExp.addData('colorChosen', [L,A,B])&amp;#10;&amp;#10;if condition ==1:&amp;#10; thisExp.addData('chosen_value', B)&amp;#10;&amp;#10;if condition == 2:&amp;#10; thisExp.addData('chosen_value', B)&amp;#10;&amp;#10;if condition == 3:&amp;#10; thisExp.addData('chosen_value', A)&amp;#10;&amp;#10;if condition == 4:&amp;#10; thisExp.addData('chosen_value', A)" valType="extendedCode"/>
